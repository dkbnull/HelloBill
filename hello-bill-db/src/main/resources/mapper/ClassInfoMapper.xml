<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.wbnull.hellobill.db.mapper.ClassInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.wbnull.hellobill.db.entity.ClassInfo">
        <id column="uuid" property="uuid"/>
        <result column="topClass" property="topClass"/>
        <result column="secondClass" property="secondClass"/>
        <result column="type" property="type"/>
        <result column="serialNo" property="serialNo"/>
        <result column="status" property="status"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        uuid
        , topClass, secondClass, type, serialNo, status
    </sql>

</mapper>
