<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.wbnull.hellobill.db.mapper.ClassInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.wbnull.hellobill.db.entity.ClassInfo">
        <id column="uuid" property="uuid"/>
        <result column="top_class" property="topClass"/>
        <result column="second_class" property="secondClass"/>
        <result column="type" property="type"/>
        <result column="serial_no" property="serialNo"/>
        <result column="status" property="status"/>
        <result column="gmt_create" property="gmtCreate"/>
        <result column="gmt_modified" property="gmtModified"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        uuid, top_class, second_class, type, serial_no, status, gmt_create, gmt_modified
    </sql>

</mapper>
